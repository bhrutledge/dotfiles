    name = Brian Rutledge
    email = brian@bhrutledge.com
    signingkey = 328C6FC0D392D822E709A1A6D411764EE1DA4B90
[init]
    defaultBranch = main
[core]
    autocrlf = input
    safecrlf = true
[fetch]
    prune = true
[pull]
    ff = only
[commit]
    gpgsign = true
[rebase]
    autoSquash = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
